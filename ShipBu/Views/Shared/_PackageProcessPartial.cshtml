@model List<PackageProcessModel>

<div class="col-md-6 mb-3">
    @foreach (var process in Model)
    {
        <div class="form-check form-switch mb-3">
            <input class="form-check-input" type="checkbox" role="switch" id="flexSwitchCheck_@process.Id">
            <label class="form-check-label" for="flexSwitchCheck_@process.Id">
                @process.Feature
            </label>
        </div>
    }
</div>
