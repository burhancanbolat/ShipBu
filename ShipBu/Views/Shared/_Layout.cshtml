@await RenderSectionAsync("Scripts", required: false)
<script>
    @if (TempData["success"] is not null)
    {
        <text>
            Swal.fire({
                toast: true,
            position: 'top',
            icon: 'success',
            title: '@TempData["success"]',
            showConfirmButton : false,
            timer : 5000
        })
        </text>
    }

    @if (TempData["error"] is not null)
    {
        <text>
            Swal.fire({
                toast: true,
            position: 'top',
            icon: 'error',
            title: '@TempData["error"]',
            showConfirmButton : false,
            timer : 5000
            })
        </text>
    }
</script>