@using ShipBu.Areas.Admin.Models
@model FeatureDetailViewModel
<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Horizontal Layout</title>

</head>
<body>
    @foreach (var genre in Model.SendingGenre)
    {
    <div class="card text-center">
        <div class="card-header">
                @genre.Name
               
        </div>
        <div class="card-body">
            <h5 class="card-title"></h5>
            <p class="card-text">Alıcı Ülke : @Model.CountryName
                <p class="card-text">Eyalet : @Model.StateName
                <p class="card-text"> Amazon Depo : @Model.wareHouseName

        </div>
        <div class="card-footer text-body-secondary">
                @genre.DeliveryTıme<br />
                <a href="#" class="btn btn-primary"> Teklifi Seç </a>
        </div>
    </div>
    <br />
    }

    <!-- Bootstrap JS and Popper.js -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>




</body>
</html>
<script>


   


</script>